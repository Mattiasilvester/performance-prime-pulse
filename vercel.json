{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "functions": {
    "api/supabase-proxy.ts": {
      "maxDuration": 30
    },
    "api/ai-chat.ts": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/supabase-proxy/(.*)",
      "destination": "/api/supabase-proxy"
    },
    {
      "source": "/api/ai-chat",
      "destination": "/api/ai-chat"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}





